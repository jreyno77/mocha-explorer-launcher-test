{
  "mochaExplorer.launcherScript": "out/customLauncher.js",
  "mochaExplorer.files": "out/test/**/*.js",
  "mochaExplorer.logpanel": true,
  "mochaExplorer.ui": "tdd",
  "mochaExplorer.require": [
    "ts-node/register",
    "tsconfig-paths/register",
    "source-map-support/register"
  ],
  "mochaExplorer.debuggerConfig": "Debug: Launcher + Extension Host",
  "mochaExplorer.env": {
    "VSCODE_VERSION": "stable",
    "ELECTRON_RUN_AS_NODE": null
  },
  "mochaExplorer.ipcRole": "server",
  "mochaExplorer.autoload": false,

  "mochaExplorer.debuggerPort": 9229,
  "mochaExplorer.ipcTimeout": 10000,
}