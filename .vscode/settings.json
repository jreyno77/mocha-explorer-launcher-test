{
  "mochaExplorer.launcherScript": "./out/customLauncher.js",
  "mochaExplorer.files": "out/test/**/*.js",
  "mochaExplorer.ui": "tdd",
  "mochaExplorer.autoload": false,
  "mochaExplorer.logpanel": true,
  "mochaExplorer.debuggerConfig": "Debug: Launcher + Extension Host",
  // Let the adapter (extension) listen, worker connects as "client"
  "mochaExplorer.ipcRole": "client",
  "mochaExplorer.ipcTimeout": 6000,

  // Env that will be forwarded by the launcher (see extensionTestsEnv above)
  "mochaExplorer.env": {
        "EXT_HOST_DEBUG_PORT": "9229",
        "VSCODE_VERSION": "insiders",
        "RUN_TESTS_FILE": "${workspaceFolder}/out/runTests.js",
        "VSCODE_WORKSPACE_PATH": "${workspaceFolder}",
  }
}
