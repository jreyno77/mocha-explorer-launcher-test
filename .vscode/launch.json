{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "1) Debug Custom Launcher",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/out/customLauncher.js",
      "cwd": "${workspaceFolder}",
      "args": ["{\"role\":\"server\",\"port\":9449,\"host\":\"localhost\"}"],
      "preLaunchTask": "npm: build",
      "skipFiles": ["<node_internals>/**"],
      "trace": true
    },
    {
      "name": "2) Debug VsCode Testing after Launch break",
      "type": "node",
      "request": "attach",
      "port": 9229,
      "autoAttachChildProcesses": true,
      "continueOnAttach": true,
      "trace": true,
      "preLaunchTask": "wait-for-9229"
    }
], "compounds": [
  {
    "name": "Debug: Launcher + Extension Host",
    "configurations": [
      "1) Debug Custom Launcher",
      "2) Debug VsCode Testing after Launch break"
    ],
    "stopAll": true
  }
]

}
